{
    "test_cases": [
        {
            "route": "/",
            "method": "GET",
            "description": "Test the home route",
            "expected_status": 200,
            "expected_response": "Hello, it App Server"
        },
        {
            "route": "/api/users/register",
            "method": "POST",
            "description": "Test user registration with valid data",
            "request_body": {
                "username": "testuser",
                "email": "test@example.com",
                "password": "password123"
            },
            "expected_status": 201,
            "expected_response": {
                "message": "User registered successfully"
            }
        },
        {
            "route": "/api/users/register",
            "method": "POST",
            "description": "Test user registration with existing email",
            "request_body": {
                "username": "testuser",
                "email": "test@example.com",
                "password": "password123"
            },
            "expected_status": 400,
            "expected_response": {
                "message": "User already exists"
            }
        },
        {
            "route": "/api/users/login",
            "method": "POST",
            "description": "Test login with valid credentials",
            "request_body": {
                "email": "test@example.com",
                "password": "password123"
            },
            "expected_status": 200,
            "expected_response": {
                "message": "Login successful"
            }
        },
        {
            "route": "/api/users/login",
            "method": "POST",
            "description": "Test login with invalid credentials",
            "request_body": {
                "email": "test@example.com",
                "password": "wrongpassword"
            },
            "expected_status": 401,
            "expected_response": {
                "message": "Invalid credentials"
            }
        },
        {
            "route": "/api/users/logout",
            "method": "POST",
            "description": "Test user logout",
            "expected_status": 200,
            "expected_response": {
                "message": "Logout successful"
            }
        },
        {
            "route": "/api/users/delete",
            "method": "DELETE",
            "description": "Test deleting an existing user",
            "request_body": {
                "email": "test@example.com"
            },
            "expected_status": 200,
            "expected_response": {
                "message": "User deleted successfully"
            }
        },
        {
            "route": "/api/users/delete",
            "method": "DELETE",
            "description": "Test deleting a non-existent user",
            "request_body": {
                "email": "nonexistent@example.com"
            },
            "expected_status": 404,
            "expected_response": {
                "message": "User not found"
            }
        },
        {
            "route": "/api/chatbot/predict",
            "method": "POST",
            "description": "Test chatbot response with a valid message",
            "request_body": {
                "message": "Hello"
            },
            "expected_status": 200,
            "expected_response": {
                "response": "Expected chatbot reply"
            }
        },
        {
            "route": "/api/chatbot/predict",
            "method": "POST",
            "description": "Test chatbot response with an empty message",
            "request_body": {},
            "expected_status": 400,
            "expected_response": {
                "error": "Message is required"
            }
        }
    ]
}